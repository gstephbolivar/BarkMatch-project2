<form class="create-form">

  <div class="form-group">
    <label for="dog">Dog Name:</label>
    <input type="text" id="dog-name" name="name">
  </div>
  <div class="form-group">
    <label for="dog">Age:</label>
    <input type="text" id="dog-age" name="name">
  </div>
  <div class="form-group">
    <label for="dog">Breed:</label>
    <input type="text" id="dog-breed" name="name">
  </div>
  <div class="form-group">
    <label for="dog">Bio:</label>
    <input type="text" id="dog-bio" name="name">
  </div>
  
  <div class="form-group">
    <label for="gender">Gender</label><br>
    <input type="radio" name="gender" data-name="Male" checked>Male<br>
    <input type="radio" name="gender" data-name="Female" > Female
  </div>
  <div class="form-group">
    <label for="size">Size</label><br>
    <input type="radio" name="size" data-name="Small" checked>Small<br>
    <input type="radio" name="size" data-name="Medium" checked>Medium<br>
    <input type="radio" name="size" data-name="Large"> Large
  </div>
  <div class="form-group">
    <label for="energy">Energy Level</label><br>
    <input type="radio" name="energy" data-name="Low" checked>Low<br>
    <input type="radio" name="energy" data-name="Moderate" checked>Moderate<br>
    <input type="radio" name="energy" data-name="High" > High
  </div>

  <button type="submit">Add Dog</button>
</form>

<script src="assets/js/addDog.js"></script>